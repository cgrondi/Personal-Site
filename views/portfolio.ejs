<%- include("partials/header"); -%>
<h1 class="page-title">Portfolio</h1>

<% projects.forEach(function(project){ %>


  <img class='project-image' src=<%= project.imgURL %> alt=<%= project.title+' image' %>>
  <div class="project">

    <h3><%= project.title %></h3>
    <p><%= project.description %></p>
    <div class="project-links">
      <a href=<%= project.codeURL %> target="_blank">Code</a> <a href=<%= project.demoURL %> target="_blank">Demo</a>
    </div>
  </div>
  <div class="gradient-border" id='border-box'>

  </div>
  <br>

  <br>


<% }) %>



<%- include("partials/footer"); -%>
